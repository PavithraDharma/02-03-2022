//setter

class Setters
{
   int id;
   String name;
   double salary;
   public void setId(int id)
   {
       this.id=id;
   }
   public void setName(String name)
   {
       this.name=name;
   }
   public void setSalary(double salary)
   {
       this.salary=salary;
   }
   public void disp()
   {
       System.out.println("Id= "+id+"\nName= "+name+"\nSalary= "+salary);
   }
   public static void main(String[] args)
   {
       Setters obj=new Setters();
       obj.setId(28);
       obj.setName("Pavi");
       obj.setSalary(30000);
       obj.disp();
   }
}
